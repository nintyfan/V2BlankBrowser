{
  "id": "pl.art.lach.slawek.apps.BlankBrowser",
  "runtime": "org.gnome.Platform",
  "runtime-version": "3.38",
  "sdk": "org.gnome.Sdk",
  "command": "BlankBrowser",
  "clear": [ "*.o"],
  "build-options" : {
    "arch": {
      "x86_64": {
      },
      "x86": {
      }
    }
  },
  "modules": [
  {
    "name": "main",
    "buildsystem" : "cmake",
    "sources": [
    {
      "type": "dir",
      "path": "."
    }
    ]
  }
  ],
  
  "finish-args" : [
  "--device=dri",
  "--share=ipc",
  "--share=network",
  "--socket=x11",
  "--socket=pulseaudio"
  ]
}
